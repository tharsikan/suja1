<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>Student</servlet-name>
		<servlet-class>com.sgic.student.controller.StudentView</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Student</servlet-name>
		<url-pattern>/view</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>registerStudent</servlet-name>
		<servlet-class>com.sgic.student.controller.RegisterStudent</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registerStudent</servlet-name>
		<url-pattern>/registerStudent</url-pattern>
	</servlet-mapping>
</web-app>